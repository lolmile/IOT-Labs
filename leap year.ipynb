    "#Emile Boulet     2023-02-09\n",
    "#This code will find if the year input is leap year\n",
    "\n",
    "year = int(input(\"Enter a year: \"))\n",
    "\n",
    "if year % 400 == 0:\n",
    "    print(\"TRUE\")\n",
    "elif year % 100 == 0:\n",
    "    print(\"FALSE\")\n",
    "elif year % 4 == 0:\n",
    "    print(\"TRUE\")\n",
    "else:\n",
    "    print(\"FALSE\")"
   ]
